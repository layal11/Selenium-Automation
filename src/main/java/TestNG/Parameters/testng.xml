<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">

    <test verbose="2" preserve-order="true" name="chromeOrAnything">
        <parameter name="browser" value="chrome"/>
        <parameter name="url" value="https://github.com/"/>

        <classes>
            <class name="TestNG.Parameters.parameters"/>
        </classes>
    </test>

    <test verbose="2" preserve-order="true" name="firefoxOrAnything">
        <parameter name="browser" value="firefox"/>
        <parameter name="url" value="https://github.com/"/>

        <classes>
            <class name="TestNG.Parameters.parameters"/>
        </classes>
    </test>

</suite>